<div class="trivia-container">
  {{#each questions as |question|}}
    <div class="question">
      <h2>{{question}}</h2>
      <div id="choices-container"></div>
    </div>
  {{/each}}
</div>
<div class="status">
    <p>Level: <span id="level">{{level}}</span></p>
    <p>Strikes: <span id="strikeCount">{{strikes}}</span></p>
</div>